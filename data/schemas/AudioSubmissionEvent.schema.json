{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AudioSubmissionEvent",
  "type": "object",
  "properties": {
    "submissionId": {
      "type": "string",
      "format": "uuid"
    },
    "childId": {
      "type": "string",
      "format": "uuid"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "lang": {
      "type": "string"
    },
    "durationSec": {
      "type": "number"
    }
  },
  "required": [
    "submissionId",
    "childId",
    "timestamp"
  ]
}