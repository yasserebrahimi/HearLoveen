{
  "title": "HearLoveen Worker",
  "panels": [
    {
      "type": "graph",
      "title": "Processed messages",
      "targets": [
        {
          "expr": "rate(worker_requests_total[5m])"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Processing latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(worker_processing_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Errors",
      "targets": [
        {
          "expr": "increase(worker_errors_total[1h])"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "version": 1
}