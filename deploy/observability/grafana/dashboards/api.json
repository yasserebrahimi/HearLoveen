{
  "title": "HearLoveen API",
  "panels": [
    {
      "type": "graph",
      "title": "Requests/sec",
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "p95 Request Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "5xx rate",
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count{status=~\"5..\"}[5m]))"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "version": 1
}